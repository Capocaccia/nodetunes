<!DOCTYPE html>
<html>
<head>
  <title>Artist Index</title>
</head>
<body>
  <h1>Artists</h1>
  <a href="/newartists">New Artist</a>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Genre</th>
        <th>Wiki</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% music.forEach(function (music) { %>
        <tr>
          <td><%= music.name %></td>
          <td><%= music.genre %></td>
          <td><a href=<%= music.wiki %>>Wikipedia</td>
          <td>
            <form action='/aindex/delete/<%=music._id %>' method="POST">
              <input type='submit' value="Delete">
            </form>
          </td>
       </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
