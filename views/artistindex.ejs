<!DOCTYPE html>
<html>
<head>
  <title>Artist Index</title>
</head>
<body>
  <h1>Artists</h1>
  <a href="/newartists">New Artist</a>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>ID</th>
      </tr>
    </thead>
    <tbody>
      <% music.forEach(function (music) { %>
        <tr>
          <td><%= music.name %></td>
          <td><%= music.genre %></td>
          <td><%= music.wiki %></td>
          <td><%= music._id %></td>
        <tr>
          <form action='/aindex/delete/<%=music._id %>' method="POST">
            <input type='submit' value="Delete">
          </form>
        </tr>
       </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
